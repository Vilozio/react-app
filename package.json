  "dependencies": {
    "react": "^15.4.2",
    "react-datepicker": "^0.41.1",
    "react-dom": "^15.4.2",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "scripts": {
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "check-coverage": "jest --config jest.coverage.json",
    "validate":
      "npm-run-all --continue-on-error --parallel validate-webpack:* lint check-coverage",
    "start": "node ./server.js",
    "debug": "webpack --env.debug",
    "debug:dev": "npm run debug -- --env.dev",
    "debug:prod": "npm run debug -- --env.prod",
    "build": "webpack --progress --env.dev",
    "build:prod": "webpack --env.prod -p",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "lint": "eslint . --ext .js,.jsx",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "clean": "rimraf node_modules"
  }
}
