{
  "collectCoverage": true,
  "collectCoverageFrom": ["test/**/*.{js,jsx}", "src/**/*.{js,jsx}"],
  "coverageDirectory": "coverage",
  "coverageReporters": ["json", "lcov", "text"],
  "coverageThreshold": {
    "global": {
      "statements": 4,
      "branches": 0,
      "lines": 5,
      "functions": 2
    }
  }
}
